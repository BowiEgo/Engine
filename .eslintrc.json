{
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 2015,
		"sourceType": "module"
	},
	"rules": {
		"indent": [
            "error",
            2,
			{
                "SwitchCase": 1,
                "VariableDeclarator": "first"
            }
		],
		"no-console": "off",
		"quotes": [
			"error",
			"single"
		]
	}
}